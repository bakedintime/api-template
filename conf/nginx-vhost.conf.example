server{
        listen ***REMOVED***.200.34:80 default;
        server_name api.seguros.com;

        root /home/administrador/webapps/seguro***REMOVED***pi;

        access_log /var/log/seguro***REMOVED***pi/access.log;
        error_log /var/log/seguro***REMOVED***pi/error.log;

        location / {
                try_files $uri @gunicorn_proxy;
        }
        location @gunicorn_proxy{
                proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
                proxy_set_header Host $http_host;
                proxy_redirect off;
                proxy_pass http://***REMOVED***.200.34:5000;
        }
}




